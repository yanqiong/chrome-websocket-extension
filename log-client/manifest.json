{
    "name": "Tianqin Debug Log Extensions",
    "description": "Show Tianqin Debug Log",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Tianqin Debug Log Extensions"
    },
    "permissions": [],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}