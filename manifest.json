{
    "name": "TqReplayTestExtensions",
    "description": "WebSocket Client Extension For Test",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "TqReplay Test Extension"
    },
    "permissions": [
        "experimental"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' blob: filesystem: "
}
